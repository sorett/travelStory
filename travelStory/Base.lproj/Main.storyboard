                                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFill" horizontalHuggingPriority="251" verticalHuggingPriority="251" fixedFrame="YES" image="g1-img3" translatesAutoresizingMaskIntoConstraints="NO" id="6G4-GR-te5">
